{
  "name": "mycoco-express-api",
  "version": "1.0.0",
  "description": "Custom Express API starterkit",
  "main": "app.js",
  "author": "Sheldyn",
  "license": "MIT",
  "scripts": {
    "babel-node": "babel-node --presets='@babel/preset-env'",
    "dev": "nodemon --exec yarn run babel-node -- ./src/server.js",
    "build": "babel -d ./build ./src",
    "start": "node build/server.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.2.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "delay": 2500,
    "env": {
      "PORT": 4000,
      "DEBUG": "mycoco:*"
    }
  }
}
